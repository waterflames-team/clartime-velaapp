<template>
  <div class="page">
    <stack style="width: 100%; height: 100%" class="horizontal-center">
      <img src="/common/assets/guide_5.png" alt="" />
      <!-- 继续课程载入 -->
      <div style="position: absolute;width: 324px;height: 80px;bottom:92px" onclick="next()"></div>
      <div style="position: absolute;width: 324px;height: 80px;bottom:6px" onclick="no()"></div>
    </stack>
  </div>
</template>
<script>
export default {
  private: {},

  next() {
    setTimeout(() => {
      this.$app.$def.router.push({
        uri: "pages/entries/guide/7",
        params: {
          ___PARAM_LAUNCH_FLAG___: "clearTask"
        }
      })
    }, 300)
  },
  no() {
    setTimeout(() => {
      this.$app.$def.router.push({
        uri: "pages/entries/guide/6",
        params: {
          ___PARAM_LAUNCH_FLAG___: "clearTask"
        }
      })
    }, 300)
  },

  onShow() {
    setTimeout(() => {
      this.$app.$def.dataManage.changeGuideSetting("guide_step", "5")
    }, 300)
  },

  onBackPress() {
    this.$app.$def.app.terminate()
    return true
  }
}
</script>

